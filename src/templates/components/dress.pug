section.dress.block 
  .container 
    .dress__content
      h2.dress__title.title dress code 
      h3.dress__subtitle bohème chic
      p.dress__text Богемна розкіш, трохи театральності, трохи <br> авангарду. Вечірні образи з блиском, фактурою, <br> мистецькою свободою. Це про стиль, <br> сміливість і&nbsp;витончену екстравагантність.
      
    picture.dress__decor.dress__decor--left
      source(type="image/webp" media="(min-width:480px)" srcset="img/dress1.webp")
      source(media="(min-width:480px)" srcset="img/dress1.png")
      source(type="image/webp" media="(min-width:320px)" srcset="img/dress1-479.webp")
      source(media="(min-width:320px)" srcset="img/dress1-479.png")
      img(src="img/dress1.png" alt="" loading="lazy" width="290" height="419" fetchpriority="low" decoding="async")  
    picture.dress__decor.dress__decor--right
      source(type="image/webp" media="(min-width:480px)" srcset="img/dress2.webp")
      source(media="(min-width:480px)" srcset="img/dress2.png")
      source(srcset="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAYAAAAfFcSJAAAADUlEQVR42mP8/5+hHgAHggJ/PchI7wAAAABJRU5ErkJggg==")
      img(src="img/dress2.png" alt="" loading="lazy" width="1" height="1" fetchpriority="low" decoding="async")  

    picture.diamond.diamond-2
      source(type="image/webp" media="(min-width:480px)" srcset="img/d11.webp")
      source(media="(min-width:480px)" srcset="img/d11.png")
      source(type="image/webp" media="(min-width:320px)" srcset="img/d11-mob.webp")
      source(media="(min-width:320px)" srcset="img/d11-mob.png")
      img(src="img/d11.png" alt="" loading="lazy" width="26" height="27" fetchpriority="low" decoding="async")
    picture.diamond.diamond-3
      source(type="image/webp" media="(min-width:480px)" srcset="img/d2.webp")
      source(media="(min-width:480px)" srcset="img/d2.png")
      source(type="image/webp" media="(min-width:320px)" srcset="img/d2-mob.webp")
      source(media="(min-width:320px)" srcset="img/d2-mob.png")
      img(src="img/d2.png" alt="" loading="lazy" width="18" height="18" fetchpriority="low" decoding="async")
